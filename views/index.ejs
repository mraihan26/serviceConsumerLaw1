<!DOCTYPE html>
<html>

<head>
  <%- include('./partial/head'); %>
</head>

<body>

  <main>
    <div class="jumbotron">
      <h1>Dummy Library Service Consumer</h1>
      <p>Here is the list of our user!</p>
    </div>
    <% users.forEach(function(user) { %>
      <p>================================</p>
      <p>Name : <%= user.name %></p>
      <p>Address : <%= user.address %></p>
      <p>Date of Birth : <%= user.dob %></p>
      <p>Gender : <%= user.gender %></p>
    <% }); %>
    <p>================================</p>
</body>

</html>